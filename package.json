{
  "name": "boozle-backend",
  "version": "1.0.0",
  "description": "Backend Application for Boozle Daily Guessing Game",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev boozle-backend/src/index.ts",
    "build": "npm run prisma:generate && tsc -p boozle-backend/tsconfig.json  && npm run copy:prisma",
    "start": "bash ./start.sh",
    "prisma:generate": "prisma generate --schema=boozle-backend/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --name init",
    "copy:prisma": "mkdir -p boozle-backend/dist/generated && cpx \"boozle-backend/src/generated/**/*\" boozle-backend/dist/generated"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eoinc134/boozle-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/eoinc134/boozle-backend/issues"
  },
  "homepage": "https://github.com/eoinc134/boozle-backend#readme",
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "axios": "^1.13.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mui": "^0.0.1",
    "node-fetch": "^3.3.2",
    "prisma": "^6.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "cpx": "^1.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
